<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- Transactional Outbox Table -->
    <changeSet author="redDot" id="9">
        <createSequence sequenceName="name_id_seq" startValue="1" incrementBy="1"/>
        <createTable tableName="name">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="title" type="VARCHAR(100)"/>
            <column name="firstName" type="VARCHAR(1000)"/>
            <column name="middleName" type="VARCHAR(1000)"/>
            <column name="lastName" type="VARCHAR(1000)"/>
            <column name="fullName" type="VARCHAR(1000)"/>
        </createTable>
    </changeSet>


</databaseChangeLog>
