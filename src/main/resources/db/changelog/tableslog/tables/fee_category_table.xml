<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- Fee Category Table -->
    <changeSet author="redDot" id="5">
        <createSequence sequenceName="fee_category_id_seq" startValue="1" incrementBy="1"/>
        <createTable tableName="fee_category">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="bill_run_id" type="BIGINT"/>
            <column name="pay_acct_code" type="VARCHAR(50)"/>
            <column name="free_unit" type="VARCHAR(50)"/>
            <column name="created_at" type="TIMESTAMP"/>
            <column name="updated_at" type="TIMESTAMP"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
